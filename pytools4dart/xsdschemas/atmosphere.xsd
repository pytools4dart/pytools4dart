			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="databaseName" type="xsd:string" default="dart_atmosphere.db">
			<xsd:annotation>
				<xsd:documentation xml:lang = "en" source="attribut"> Atmospheric database name</xsd:documentation> 
				<xsd:documentation xml:lang = "fr" source="attribut"> Atmospheric database name</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="correctionBandModel" type="xsd:int" default="1">
			<xsd:annotation>
				<xsd:documentation xml:lang = "en" source="attribut"> Band model correction</xsd:documentation> 
				<xsd:documentation xml:lang = "fr" source="attribut"> Band model correction</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="temperatureModelName" type="xsd:string" default="USSTD76">
			<xsd:annotation>
				<xsd:documentation xml:lang = "en" source="attribut"> Gas temperature model</xsd:documentation> 
				<xsd:documentation xml:lang = "fr" source="attribut"> Gas temperature model</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="redefTemperature" type="xsd:int" default="0">
			<xsd:annotation>
				<xsd:documentation xml:lang = "en" source="attribut"> Redefine temperature profile up to 10km</xsd:documentation> 
				<xsd:documentation xml:lang = "fr" source="attribut"> Redefine temperature profile up to 10km</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="gasModelName" type="xsd:string" default="USSTD76">
			<xsd:annotation>
				<xsd:documentation xml:lang = "en" source="attribut"> Gas model</xsd:documentation> 
				<xsd:documentation xml:lang = "fr" source="attribut"> Gas model</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="gasGroup" type="xsd:int" default="1">
			<xsd:annotation>
				<xsd:documentation xml:lang = "en" source="attribut"> Same model for gas parameters </xsd:documentation> 
				<xsd:documentation xml:lang = "fr" source="attribut"> Same model for gas parameters </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="gasCumulativeModelName" type="xsd:string" default="USSTD76">
			<xsd:annotation>
				<xsd:documentation xml:lang = "en" source="attribut"> Gas cumulative model</xsd:documentation> 
				<xsd:documentation xml:lang = "fr" source="attribut"> Gas cumulative model</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="ignoreGasForExtrapolation" type="xsd:int" default="0">
			<xsd:annotation>
				<xsd:documentation xml:lang = "en" source="attribut"> Gas -- Ignore gas for atmosphere extrapolation</xsd:documentation> 
				<xsd:documentation xml:lang = "fr" source="attribut"> Gas -- Ignore gas for atmosphere extrapolation</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="co2MixRate" type="xsd:double" default="365.0">
			<xsd:annotation>
				<xsd:documentation xml:lang = "en" source="attribut"> CO2 mixing rate</xsd:documentation> 
				<xsd:documentation xml:lang = "fr" source="attribut"> CO2 mixing rate</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="scaleOtherGases" type="xsd:int" default="0">
			<xsd:annotation>
				<xsd:documentation xml:lang = "en" source="attribut"> Manually define the amount of other gases</xsd:documentation> 
				<xsd:documentation xml:lang = "fr" source="attribut"> Manually define the amount of other gases</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>

