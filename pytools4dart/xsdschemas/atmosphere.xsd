			<xsd:element name="tempParameters" type="_tempParameters" minOccurs="1" maxOccurs="1" />
		</xsd:sequence>
		<xsd:attribute name="precipitableWaterAmountCkeckbox" type="xsd:int" default="0">
			<xsd:annotation>
				<xsd:documentation xml:lang = "en" source="attribut"> Define water vapour amount                         </xsd:documentation> 
