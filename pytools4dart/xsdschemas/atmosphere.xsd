			<xsd:documentation xml:lang = "fr"> Fill in defined water vapor profile</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="h2oDef" type="_h2oDef" minOccurs="1" maxOccurs="1" />
		</xsd:sequence>
		<xsd:attribute name="ifFillH2O" type="xsd:int" default="0">
			<xsd:annotation>
				<xsd:documentation xml:lang = "en" source="attribut"> Define water vapor parameters at a point</xsd:documentation> 
				<xsd:documentation xml:lang = "fr" source="attribut"> Define water vapor parameters at a point</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>

	<xsd:complexType name="_h2oDef">
		<xsd:annotation>
			<xsd:documentation xml:lang = "en"> Parameters</xsd:documentation> 
			<xsd:documentation xml:lang = "fr"> Parameters</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
		</xsd:sequence>
		<xsd:attribute name="pressure" type="xsd:double" default="1013">
			<xsd:annotation>
				<xsd:documentation xml:lang = "en" source="attribut"> Pressure</xsd:documentation> 
