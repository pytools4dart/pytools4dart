			<xsd:documentation xml:lang = "en"> Building</xsd:documentation> 
			<xsd:documentation xml:lang = "fr"> Building</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="WallProperties" type="_WallProperties" minOccurs="1" maxOccurs="1" />
