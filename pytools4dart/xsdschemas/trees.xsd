				<xsd:documentation xml:lang = "en" source="attribut"> Name of the file that stores the position of the trees. This file is created if it is specified as unavailable </xsd:documentation> 
				<xsd:documentation xml:lang = "fr" source="attribut"> Name of the file that stores the position of the trees. This file is created if it is specified as unavailable </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>

	<xsd:complexType name="_Specie_2">
		<xsd:annotation>
			<xsd:documentation xml:lang = "en"> </xsd:documentation> 
			<xsd:documentation xml:lang = "fr"> </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="OpticalPropertyLink" type="_OpticalPropertyLink" minOccurs="1" maxOccurs="1" />
			<xsd:element name="ThermalPropertyLink" type="_ThermalPropertyLink" minOccurs="1" maxOccurs="1" />
			<xsd:element name="Trunk" type="_Trunk" minOccurs="1" maxOccurs="1" />
			<xsd:element name="Crown" type="_Crown" minOccurs="1" maxOccurs="1" />
			<xsd:element name="CrownLevel" type="_CrownLevel" minOccurs="1" maxOccurs="unbounded" />
			<xsd:element name="BranchesAndTwigs" type="_BranchesAndTwigs" minOccurs="1" maxOccurs="1" />
		</xsd:sequence>
		<xsd:attribute name="branchesAndTwigsSimulation" type="xsd:int" default="0">
			<xsd:annotation>
				<xsd:documentation xml:lang = "en" source="attribut"> Simulation of trees with branches and twigs</xsd:documentation> 
				<xsd:documentation xml:lang = "fr" source="attribut"> Simulation of trees with branches and twigs</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="lai" type="xsd:double" default="4.00">
			<xsd:annotation>
				<xsd:documentation xml:lang = "en" source="attribut"> LAI </xsd:documentation> 
				<xsd:documentation xml:lang = "fr" source="attribut"> LAI </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>

	<xsd:complexType name="_Trunk">
		<xsd:annotation>
			<xsd:documentation xml:lang = "en"> Trunk</xsd:documentation> 
			<xsd:documentation xml:lang = "fr"> Trunk</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
		</xsd:sequence>
