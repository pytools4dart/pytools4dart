	<xsd:complexType name="_Coeff_diff">
		<xsd:annotation>
			<xsd:documentation xml:lang = "en"> </xsd:documentation> 
			<xsd:documentation xml:lang = "fr"> </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="FluorescenceProductsProperties" type="_FluorescenceProductsProperties" minOccurs="1" maxOccurs="1" />
			<xsd:element name="LambertianMultiFunctions" type="_LambertianMultiFunctions" minOccurs="1" maxOccurs="1" />
			<xsd:element name="HapkeSpecularMultiFunctions" type="_HapkeSpecularMultiFunctions" minOccurs="1" maxOccurs="1" />
