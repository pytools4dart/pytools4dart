			<xsd:element name="AirMultiFunctions" type="_AirMultiFunctions" minOccurs="1" maxOccurs="1" />
			<xsd:element name="Temperatures" type="_Temperatures" minOccurs="1" maxOccurs="1" />
		</xsd:sequence>
		<xsd:attribute name="fluorescenceFile" type="xsd:int" default="0">
			<xsd:annotation>
				<xsd:documentation xml:lang = "en" source="attribut"> Define yields (PSI, PSII) per triangle - in the order generated by maket.            </xsd:documentation> 
				<xsd:documentation xml:lang = "fr" source="attribut"> Define yields (PSI, PSII) per triangle - in the order generated by maket.            </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="fluorescenceProducts" type="xsd:int" default="0">
			<xsd:annotation>
				<xsd:documentation xml:lang = "en" source="attribut"> Elements which have been defined as fluorescent will fluoresce. Separated images and BRFs will be generated for Photo-System I and II.</xsd:documentation> 
				<xsd:documentation xml:lang = "fr" source="attribut"> Elements which have been defined as fluorescent will fluoresce. Separated images and BRFs will be generated for Photo-System I and II.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>

	<xsd:complexType name="_FluorescenceProductsProperties">
